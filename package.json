{
  "name": "@interactive-svg/library",
  "version": "0.0.8",
  "description": "A library for creating interactive images.",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "http-server": "^0.11.1",
    "mocha": "^6.1.4",
    "rollup": "^1.15.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.1"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "http-server .",
    "docs": "typedoc",
    "module": "tsc && cp css/library.css dist/library.css",
    "package": "cp css/library.css dist/library.css && tar -czvf library.tar.gz dist",
    "rollup": "rollup --config",
    "prep": "tsc && npm run docs && npm run package && npm run rollup"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://capstone-cs.eng.utah.edu/interactive-svg/library.git"
  },
  "keywords": [
    "svg",
    "interactive",
    "visualization",
    "dom"
  ],
  "contributors": [
    "Kurt Bruns",
    "Derek Castillo",
    "Zach Cutler",
    "Josh Homer"
  ],
  "license": "ISC",
  "main": "dist/Library.bundle.js",
  "module": "dist/Interactive.js",
  "modules.root": "dist"
}
